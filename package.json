{
  "name": "portfolio",
  "packageManager": "yarn@4.1.1",
  "source": "src",
  "outDir": "dist",
  "type": "module",
  "author": "Mike Earley",
  "license": "THE UNLICENSE",
  "description": "My personal portfolio site",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel watch:css watch:esbuild",
    "css": "NODE_ENV=development tailwindcss -i src/index.css -o dist/index.css",
    "watch:esbuild": "NODE_ENV=development node ./esbuild.config.js --watch",
    "watch:css": "NODE_ENV=development tailwindcss -i src/index.css -o dist/index.css --watch",
    "build": "NODE_ENV=production npm-run-all --parallel build:esbuild build:css",
    "build:esbuild": "NODE_ENV=production node ./esbuild.config.js",
    "build:css": "NODE_ENV=production tailwindcss -i src/index.css -o dist/index.css"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@icons-pack/react-simple-icons": "^9.4.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15",
    "autoprefixer": "^10.4.19",
    "browserslist": "^4.23.0",
    "esbuild": "^0.20.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-markdown": "^0.0.3",
    "esbuild-plugin-tailwind": "^0.1.0",
    "flatted": "^3.3.1",
    "globals": "^15.0.0",
    "inter-ui": "^4.0.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-syntax-highlighter": "^15.5.0",
    "styled-components": "^6.1.8",
    "tailwindcss": "^3.4.3",
    "tinybuild": "^0.10.12",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
